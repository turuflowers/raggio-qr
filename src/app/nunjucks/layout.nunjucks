<!doctype html>
<html lang="{{ locale }}">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="format-detection" content="telephone=no">

    {% block meta_head %}{% endblock %}

    <!-- Place favicon.ico in the root directory -->
    <link rel="icon" type="image/png" href="/images/favicon.png">

    {#<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">#}
    {#<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">#}
    {#<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">#}

    <meta name="theme-color" content="#fafafa">

    <!-- build:css /styles/vendor.css -->
    <!-- bower:css -->
    <link rel="stylesheet" href="/bower_components/css-common/dist/common.min.css" />
    <link rel="stylesheet" href="/bower_components/components-font-awesome/css/all.css" />
    <!-- endbower -->
    <!-- endbuild -->

    <!-- build:css /styles/main.css -->
    <link rel="stylesheet" media="screen" type="text/css"  href="/styles/main.css">
    <!-- endbuild -->

    {% block page_styles %}{% endblock %}

    {% block head_retail %}{% endblock %}

    {% block schema %}{% endblock %}
</head>
{% block content %} {% endblock %}



<!-- build:js /scripts/vendor.js -->
<!-- bower:js -->
<script src="/bower_components/jquery/dist/jquery.js"></script>
<script src="/bower_components/wow/dist/wow.js"></script>
<!-- endbower -->
<!-- endbuild -->
{% block scripts_retail %}{% endblock %}
{% block page_scripts %}{% endblock %}
<div id="blackout" class=""></div>

</body>
</html>
